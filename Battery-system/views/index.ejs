    <div class="row px-xl-5">
        <div class="col-lg-8">
            <div id="header-carousel" class="carousel slide carousel-fade mb-30 mb-lg-0" data-ride="carousel">
                <ol class="carousel-indicators">
                    <% slides.forEach((slide, index) => { %>
                        <li data-target="#header-carousel" data-slide-to="<%= index %>" class="<%= index === 0 ? 'active' : '' %>"></li>
                    <% }); %>
                </ol>
                <div class="carousel-inner">
                    <% slides.forEach((slide, index) => { %>
                        <div class="carousel-item position-relative <%= index === 0 ? 'active' : '' %>" style="height: 430px;">
                            <img class="img-fluid w-100 h-100" src="/img/<%= slide.image %>" name="image" style="object-fit: cover; overflow: hidden;">
                            <div class="carousel-caption d-flex flex-column align-items-center justify-content-center">
                                <div class="p-3" style="max-width: 700px;">
                                    <h1 class="display-4 text-white mb-3 animate__animated animate__fadeInDown" name="title"><%= slide.title %></h1>
                                    <p class="mx-md-5 px-5 animate__animated animate__bounceIn" name="description"><%= slide.description %></p>
                                    <a class="btn btn-outline-light py-2 px-4 mt-3 animate__animated animate__fadeInUp" href="<%= slide.button1_link %>" name="button1_text"><%= slide.button1_text %></a>
                                    <a class="btn btn-outline-light py-2 px-4 mt-3 animate__animated animate__fadeInUp" href="<%= slide.button2_link %>" name="button2_text"><%= slide.button2_text %></a>
                                </div>
                            </div>
                        </div>
                    <% }); %>
                </div>
            </div>
        </div>
        <div class="col-lg-4">
            <% headOffer.slice(-2).forEach(offer => { %>
                <div class="product-offer mb-30" style="height: 200px;">
                    <img class="img-fluid" src="/img/<%= offer.image %>" name="image" alt="">
                    <div class="offer-text">
                        <h6 class="text-white text-uppercase" name="offer_text"><%= offer.offer_text %></h6>
                        <h3 class="text-white mb-3"><%= offer.offer_subtitle %></h3>
                        <a href="<%= offer.button_link %>" class="btn btn-primary" name="button_text"><%= offer.button_text %></a>
                    </div>
                </div>
            <% }); %>
        </div>
    </div>
    

    <!-- Carousel End -->


